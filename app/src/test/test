'use strict';

var test = require('unit.js');
var index = require('../index.js');

describe('Test', function(){
    it('Successfull build!', function(check){
        index.get({},{},(err,result) => {
            try{
                test.number(result.statusCode).is(200);
                check();

            }catch(e){
                check(e);
            }

        });
    });
});
